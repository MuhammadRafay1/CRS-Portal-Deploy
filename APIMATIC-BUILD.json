{
  "$schema": "https://titan.apimatic.io/api/build/schema",
  "buildFileVersion": "1",
  "generatePortal": {
    "debug": {
      "publishReport": true
    },
    "tailIncludes": "<script defer src='./static/scripts/guide.js'></script><script>document.addEventListener('DOMContentLoaded', (event) => {APIMaticDevPortal.ready(({ registerWorkflow }) => {registerWorkflow('page:api-recipes/guides','Reports Workflow',myWorkflowDefinition);});});</script><script defer src='./static/scripts/guide2.js'></script><script>document.addEventListener('DOMContentLoaded', (event) => {APIMaticDevPortal.ready(({ registerWorkflow }) => {registerWorkflow('page:api-recipes/oauth2','OAUTH Workflow',myOauthWorkflowDefinition);});});</script>",
    "pageTitle": "CRS Portal",
    "navTitle": "CRS Developer Portal",
    "faviconUrl": "static/images/favicon.png",
    "logoUrl": "static/images/logo-light.png",
    "logoUrlDark": "static/images/logo.png",
    "useHostedPortalScript": true,
    "languageConfig": {
      "http": {},
      "csharp": {},
      "java": {},
      "typescript": {},
      "php": {},
      "python": {},
      "ruby": {}
    },
    "portalSettings": {
      "container": "apimatic-widget",
      "portalStyle": "default",
      "baseUrl": "./",
      "enableExport": true,
      "enableConsoleCalls": true,
      "enableAnalytics": true,
      "useProxyForConsoleCalls": true,
      "languageSettings": {
        "http_curl_v1": {},
        "cs_net_standard_lib": {
          "disableSdkDownload": false
        },
        "java_eclipse_jre_lib": {
          "disableSdkDownload": false
        },
        "php_generic_lib_v2": {
          "disableSdkDownload": false
        },
        "python_generic_lib": {
          "disableSdkDownload": false
        },
        "ruby_generic_lib": {
          "disableSdkDownload": false
        },
        "ts_generic_lib": {
          "disableSdkDownload": false
        }
      },
      "theme": {
        "baseTheme": "standard",
        "colors": {
          "primaryColor": {
            "light": "#A159CF",
            "dark": "#A159CF"
          },
          "linkColor": {
            "light": "#D98397",
            "dark": "#D98397"
          }
        },
        "colorMode": {
          "defaultMode": "system",
          "disableSwitch": false
        },
        "fontSource": ["https://fonts.googleapis.com/css?family=Open+Sans"],
        "cssStyles": {
          "headings": {
            "fontFamily": "Rubik, sans-serif",
            "h1": {
              "fontFamily": "Rubik, sans-serif",
              "fontSize": "27px",
              "fontWeight": "500",
              "fontStyle": "normal",
              "lineHeight": "1.3"
            }
          },
          "body": {
            "fontFamily": "Rubik, sans-serif",
            "text1": {
              "fontFamily": "Rubik, sans-serif",
              "fontSize": "15px",
              "fontWeight": "400",
              "fontStyle": "normal",
              "lineHeight": "1.75"
            }
          },
          "code": {
            "fontFamily": "'SFMono-Regular', Consolas, 'Liberation Mono', Menlo, Courier",
            "blockCode": {
              "fontFamily": "'SFMono-Regular', Consolas, 'Liberation Mono', Menlo, Courier",
              "fontSize": "15px",
              "fontWeight": "400",
              "fontStyle": "normal",
              "lineHeight": "1.75"
            }
          }
        }
      }
    }
  }
}
